.container {
  background: #F7F8FA;
  min-height: calc(100% - 180rpx);
  margin-bottom: 100rpx;
  padding-bottom: 170rpx;
}
.container .bigTitle {
  padding: 0 36rpx;
  line-height: 80rpx;
  font-weight: 500;
  font-size: 34rpx;
}
.container .list .value-class {
  flex: none !important;
}
.container .list .van-cell {
  align-items: center;
}
.container .list .van-cell .date {
  font-weight: 500;
}
.container .list .van-cell .numBox {
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding-right: 36rpx;
}
.container .list .van-cell .numBox .price {
  color: red;
}
.container .list .van-cell .numBox text {
  font-weight: 500;
}
.container .paywayBox {
  margin-top: 36rpx;
}
.container .paywayBox .paywayList image {
  width: 50rpx;
  height: 50rpx;
  padding: 10rpx 10rpx;
}
.container .paywayBox .paywayList .bankNum {
  display: flex;
  align-items: center;
}
.container .paywayBox .paywayList .bankNum .van-cell {
  padding-top: 0px;
  padding-bottom: 0px;
}
.container .paywayBox .paywayList .bankNum .bankNumInput {
  width: calc(100% - 100rpx);
}



.Toptitle{
  text-align: center;
  margin: 60rpx auto 46rpx;
  font-size: 40rpx;
  font-weight: 600;
}
.content{
  /* width: 660rpx; */
  padding:0 45rpx;
  display: flex;
  justify-content: space-around;
  align-items: center;
  margin-top: 100rpx;
}
.iptbox{
  width: 110rpx;
  height: 96rpx;
  border:1rpx solid #ddd;
  box-sizing: border-box;
  display: flex;
  justify-content: center;
  align-items: center;
  text-align: center;
}
.ipt{
  width: 0;
  height: 0;
}
.btn-area{
  width: 80%;
  margin-top: 60rpx;
}
