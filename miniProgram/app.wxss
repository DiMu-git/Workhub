/**app.wxss**/
page {
  height: 100%;
  --themeColor: #2793FF;
  --activeFontSize: 36rpx;
  color: #333333;
}
input {
  display: inline-block;
  background-color: #ffffff;
  padding: 2px 5px;
  color: #000000;
  font-size: 17px;
  height: 25px;
  box-sizing: content-box;
}
.container {
  padding-bottom: 80rpx;
  font-family: Georgia, 'Times New Roman', Times, serif;
}
.container .ellipsis2 {
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  line-clamp: 2;
  -webkit-box-orient: vertical;
}
.container .noData {
  text-align: center;
  margin-top: 200rpx;
}
.container .noData image {
  width: 510rpx;
  height: 368rpx;
}
.container .noData .words {
  margin-top: -110rpx;
  color: #a7a7a7;
  line-height: 50rpx;
}
.container .noData .words .text {
  color: #1989fa;
  margin: 0rpx 8rpx;
  display: inline-block;
}
.container .noMore {
  font-size: 24rpx;
  color: #666666;
  text-align: center;
  line-height: 50rpx;
  margin-top: 40rpx;
}
