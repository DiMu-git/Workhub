.container .bigTitle {
  padding: 0rpx 36rpx;
  font-size: 34rpx;
  font-weight: 500;
}
.container .listBox {
  display: flex;
  flex-wrap: wrap;
}
.container .listBox .item {
  width: 333rpx;
  height: 150rpx;
  border: 1rpx solid #7B7876;
  border-radius: 10rpx;
  margin: 20rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-evenly;
  color: #7B7876;
}
.container .listBox .active {
  border: 1rpx solid #3F96E7;
  color: #3F96E7;
}
.container .total {
  padding: 36rpx;
  font-weight: 500;
}
.container .total text {
  color: red;
}
.container .saveContent {
  padding: 0px 36rpx;
}
